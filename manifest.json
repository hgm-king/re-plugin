{
  "name": "Ostrich",
  "description": "Speed up your workflow!",
  "version": "0.0.8",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "/images/ostrich.png"
    }
  },
  "icons": {
    "128": "/images/ostrich.png"
  },
  "web_accessible_resources": [
    {
      "resources": [ "ga.js" ],
      "matches": [ "https://*/*" ]
    }
  ],
  "options_page": "options.html"
}
